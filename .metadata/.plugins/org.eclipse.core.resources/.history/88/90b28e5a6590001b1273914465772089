package com.javalec.base;

public class Variable_01 {
	
	public static void main(String[] args) {
		// 변수 (Variable)

		// 문자열 변수 선언
		String str = "Hello World!"; // " = " 는 할당 연산자 이다.
		System.out.println(str);
		
		str = "New";
		System.out.println(str);
		
		// 정수 변수 선언
		int num1 = 10;
		
	}
}
